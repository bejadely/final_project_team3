<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.trip.finalProject.login.mapper.NaverLoginMapper">
  <!--JSONObject 타입의 id="findNaver"  -->
  <select id="findNaver" parameterType="java.util.HashMap" resultType="MemberVO">
		select * 
		from member 
		where  member_id=#{id}
</select>

<insert id="insertNaverLogin" parameterType="java.util.HashMap">
		insert into 
		member(member_id, member_name , BIRTH_DATE,GENDER )
			values
			(#{id}, '1', '940426','1')
	</insert>
  
  </mapper>
  