<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.trip.finalProject.login.mapper.NaverLoginMapper">
  <!--JSONObject 타입의 id="findNaver"  -->
  <select id="findNaver" parameterType="java.util.HashMap" resultType="MemberVO">
		select * 
		from member 
		where  member_id=#{id}
</select>

<insert id="insertNaverLogin" parameterType="java.util.HashMap">
	<!-- 	insert into 
		member(member_id, member_name , BIRTH_DATE,GENDER, sign_up_date )
			values
			(#{id}, '1', '940426','G1', sysdate) -->
				insert into member
					(member_id,
					<!-- password, -->
					member_Name,
					Email,
					saved_profile_img,
					AUTHORITY,
			<!-- 		access_token,
					refresh_token, -->
					login_type,
					birth_date,
					gender,
					sign_up_date)
		values
					(#{id},
				<!-- 	#{password}, -->
					#{nickname},
					#{email},					
					#{profileImage},
					'A1',
				<!-- 	#{accessToken},
					#{refreshToken}, -->
					'L3',
					null,
					null,
					sysdate)
	</insert>
  
  </mapper>
  