<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="com.trip.finalProject.admin.mapper.AdminMemberMapper">
  		<!-- 권한 승인 요청 회원 전체 조회 -->
  		<select id="selectAllAuthRequest" resultType="AdminMemberVO">
  			SELECT member_id
  			     , member_name
  			     , origin_profile
  			     , save_profile
  			     , bank_name
  			     , account_number
  			  FROM member
  			 WHERE authority = 'A4'
  		  ORDER BY member_id DESC
  		</select>
  </mapper>