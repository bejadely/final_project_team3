<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trip.finalProject.common.mapper.CommonMapper">
	<select id="selectCode">
		SELECT detail_code "detailCode", 
		value "value"
		FROM detail_code
		WHERE main_code=#{mainCode}
	</select>
	


</mapper>